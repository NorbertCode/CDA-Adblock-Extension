{
  "manifest_version": 3,
  "name": "CDA Adblock",
  "description": "Block ads on cda.pl",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.cda.pl/video/*"],
      "js": ["adblocker.js"]
    }
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
